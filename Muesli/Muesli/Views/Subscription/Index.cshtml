@model IEnumerable<Muesli.Models.Subscription>

@{
    ViewBag.Title = "Subscriptions - Muesli Madness";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Date_created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubscriptionId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Delivery_frequency)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.Date_created)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubscriptionId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Delivery_frequency) day(s)
        </td>
        <td>
            € @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SubscriptionId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SubscriptionId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SubscriptionId })
        </td>
    </tr>
}

</table>
